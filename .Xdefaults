! hard contrast: 
/* *background: #1d2021 */
! medium contrast:
*background: #282828
! soft contrast: 
! *background: #32302f

*foreground: #ebdbb2
! Black + DarkGrey
*color0:  #282828
*color8:  #928374
! DarkRed + Red
*color1:  #cc241d
*color9:  #fb4934
! DarkGreen + Green
*color2:  #98971a
*color10: #b8bb26
! DarkYellow + Yellow
*color3:  #d79921
*color11: #fabd2f
! DarkBlue + Blue
*color4:  #458588
*color12: #83a598
! DarkMagenta + Magenta
*color5:  #b16286
*color13: #d3869b
! DarkCyan + Cyan
*color6:  #689d6a
*color14: #8ec07c
! LightGrey + White
*color7:  #a89984
*color15: #ebdbb2

! muudab d체naamiliselt font size
! 체leval peab URxvt.perl-ext-common: ...,font-size

URxvt.keysym.C-Up:     font-size:increase
URxvt.keysym.C-Down:   font-size:decrease
URxvt.keysym.C-S-Up:   font-size:incglobal
URxvt.keysym.C-S-Down: font-size:decglobal
URxvt.keysym.C-equal:  font-size:reset
URxvt.keysym.C-slash:  font-size:show

! keyboard-select
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.keysym.M-s: perl:keyboard-select:search


URxvt.keysym.C-1: command:\033]710;-*-tamzenforpowerline-medium-*-*-*-13-*-*-*-*-*-*-*\007\033]711;-*-tamzenforpowerline-bold-*-*-*-13-*-*-*-*-*-*-*\007
URxvt.keysym.C-2: command:\033]710;xft:Source Code Pro for Powerline:size=9:antialias=true\007\033]711;xft:Source Code Pro for Powerline:size=9:antialias=true\007
